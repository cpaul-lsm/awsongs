{
	"compilerOptions": {
	  "module": "esnext",
	  "target": "esnext",
	  "moduleResolution": "bundler",
	  "strict": true,
	  "noEmit": true,
	  "isolatedModules": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "baseUrl": ".",
	  "paths": {
		"$app/*": ["./.svelte-kit/types/$app/*"],
		"$lib/*": ["src/lib/*"]
	  }
	},
	"include": ["src/**/*", ".svelte-kit/types/**/$types.d.ts"],
	"exclude": ["node_modules", ".svelte-kit/output"]
  }